{
	"name": "hocvnu",
	"author": "betothewizard",
	"private": true,
	"version": "2.0.0",
	"type": "module",
	"scripts": {
		"dev": "react-router",
		"build": "react-router build",
		"start": "react-router-serve ./build/server/index.js",
		"scan": "react-scan",
		"deploy:app": "react-router build && wrangler pages deploy build/client --project-name hocvnu",
		"deploy:worker": "wrangler deploy workers/index.ts --name hocvnu-worker",
		"typecheck": "react-router typegen && tsc",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "wrangler d1 migrations apply hocvnu-db --remote",
		"db:migrate-local": "wrangler d1 migrations apply hocvnu-db --local",
		"db:seed": "wrangler d1 execute hocvnu-db --file=./seed.sql",
		"db:studio": "drizzle-kit studio",
		"dev:worker": "wrangler dev workers/index.ts",
		"db:insight": "wrangler d1 insights hocvnu-db --sortBy reads --timePeriod 7d"
	},
	"dependencies": {
		"@bprogress/core": "^1.3.4",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-dropdown-menu": "^2.1.16",
		"@radix-ui/react-label": "^2.1.7",
		"@radix-ui/react-navigation-menu": "^1.2.14",
		"@radix-ui/react-radio-group": "^1.3.8",
		"@radix-ui/react-slot": "^1.2.3",
		"@react-router/node": "^7.9.3",
		"@tanstack/react-query": "^5.90.2",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.44.6",
		"hono": "^4.9.9",
		"isbot": "^5.1.31",
		"lucide-react": "^0.519.0",
		"posthog-js": "^1.270.1",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-dropzone": "^14.3.8",
		"react-router": "^7.9.3",
		"react-scan": "^0.3.6",
		"tailwind-merge": "^3.3.1",
		"zustand": "^5.0.8"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20251004.0",
		"@react-router/dev": "^7.9.3",
		"@tailwindcss/vite": "^4.1.14",
		"@tanstack/eslint-plugin-query": "^5.91.0",
		"@types/node": "^22.18.8",
		"@types/react": "^19.2.0",
		"@types/react-dom": "^19.2.0",
		"better-sqlite3": "^11.10.0",
		"drizzle-kit": "^0.31.5",
		"react-router-devtools": "^5.1.3",
		"tailwindcss": "^4.1.14",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"vite": "^6.3.6",
		"vite-tsconfig-paths": "^5.1.4",
		"wrangler": "^4.42.0"
	},
	"packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
