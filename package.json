{
  "name": "hocvnu",
  "author": "betothewizard",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "react-router",
    "build": "react-router build",
    "start": "react-router-serve ./build/server/index.js",
    "scan": "react-scan",
    "deploy:app": "react-router build && wrangler pages deploy --project-name hocvnu",
    "deploy:worker": "wrangler deploy workers/index.ts --name hocvnu-worker",
    "typecheck": "react-router typegen && tsc",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply hocvnu-db --remote",
    "db:migrate-local": "wrangler d1 migrations apply hocvnu-db --local",
    "db:seed": "wrangler d1 execute hocvnu-db --file=./seed.sql",
    "db:studio": "drizzle-kit studio",
    "dev:worker": "wrangler dev workers/index.ts --x-remote-bindings",
    "db:insight": "wrangler d1 insights hocvnu-db --sortBy reads --timePeriod 7d"
  },
  "dependencies": {
    "@bprogress/core": "^1.3.4",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-router/node": "^7.8.2",
    "@tanstack/react-query": "^5.86.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.5",
    "hono": "^4.9.6",
    "isbot": "^5.1.30",
    "lucide-react": "^0.519.0",
    "posthog-js": "^1.261.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router": "^7.8.2",
    "react-scan": "^0.3.6",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250904.0",
    "@react-router/dev": "^7.8.2",
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/eslint-plugin-query": "^5.86.0",
    "@types/node": "^22.18.1",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "better-sqlite3": "^11.10.0",
    "drizzle-kit": "^0.31.4",
    "react-router-devtools": "^5.1.3",
    "tailwindcss": "^4.1.12",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5.9.2",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.34.0"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
